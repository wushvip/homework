#mysql配置信息
driver=com.mysql.jdbc.Driver
user=bcse
password=123456
url=jdbc:mysql://10.133.5.145:3306/bcse_data?useUnicode=true&characterEncoding=UTF-8

#apache日志地址
log_dir=/etc/httpd/logs/

#hdfs上的项目根路径
hdfs_home=/user/root/bcse/

#本地的项目根目录
project_home=/usr/lib/bc-se/bcselog/

#jar包名
jar_name=bcselog.jar

#日志分析结果本地目录
logoutput_path=/usr/lib/bc-se/bcselog/data/logoutput/

#定时任务配置
quartz_job_name=bcselogJob
quartz_trigger_name=bcselogTrigger
quartz_group=bcselog
#主节点cronExpression表达式,每天凌晨1点执行一次
quartz_cron_master=0 0 1 * * ? 
#从节点cronExpression表达式,每天凌晨1点30执行一次，注意：从节点的执行时间必须在主节点开始执行之后的1个小时之内
quartz_cron_slave=0 30 1 * * ?

#reids地址及端口
REDIS_SERVER_IP=10.133.17.74
REDIS_SERVER_PORT=6379
#HOTWORD_KEY的值参考mysql中热词表HOTWORDS_DAY、HOTWORDS_MONTH、HOTWORDS_WEEK中触发器保存到redis里的key
HOTWORD_KEY=HOTWORD



		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
			    <groupId>org.codehaus.mojo</groupId>
			    <artifactId>buildnumber-maven-plugin</artifactId>
			    <version>1.4</version>
			    <configuration>
			        <timestampFormat>yyyyMMddHHmmss</timestampFormat>
			    </configuration>
			    <executions>
			        <execution>
			            <goals>
			                <goal>create-timestamp</goal>
			            </goals>
			        </execution>
			    </executions>
			    <inherited>false</inherited>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<version>1.4</version>
				<executions>
					<!-- 指定需要maven install的类 -->
					<execution>
						<id>add-source</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>add-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>cmcc/bcse/search/src/java</source>
								<source>cmcc/bcse/index/src/java</source>
								<source>cmcc/bcse/tool/src/java</source>
								<source>cmcc/bcse/evaluate/src/java</source>
								<source>cmcc/bcse/datastatistics/src/java</source>
								<source>cmcc/bcse/config/src/java</source>
								<source>cmcc/bcse/app/src/java</source>
								<source>cmcc/bcse/user/src/java</source>
								<source>cmcc/bcse/validate</source>
								<source>cmcc/bcse/baseTest</source>
							</sources>
						</configuration>
					</execution>
					<!-- 指定需要maven test的单元测试类 -->
					<execution>
						<id>add-test-source</id>
						<phase>generate-test-sources</phase>
						<goals>
							<goal>add-test-source</goal>
						</goals>
						<configuration>
							<sources>
 						    	<source>cmcc/bcse/baseTest</source>
								<source>cmcc/bcse/index/src/test</source>
								<source>cmcc/bcse/search/src/test</source>
								<source>cmcc/bcse/config/src/test</source>
								<source>cmcc/bcse/datastatistics/src/test</source>
								<source>cmcc/bcse/evaluate/src/test</source>
								<source>cmcc/bcse/user/src/test</source>
								<source>cmcc/bcse/app/src/test</source>
							</sources>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>

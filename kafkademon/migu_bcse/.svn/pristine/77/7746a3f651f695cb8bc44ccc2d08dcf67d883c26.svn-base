}
.btn-lg {
  padding: 15px 45px;
  font-size: 16px;
  color: #ffffff;
  font-weight: 600;
}
.btn90 {
  padding-top: 4px;
  padding-bottom: 4px;
  width: 90px;
  font-size: 13px;
  background: 0 0;
  border-radius: 4px;
  border-width: 1px;
  border-style: solid;
}
.underline-btn,
a.underline-btn {
  padding: 0;
  border-radius: 0;
  border-bottom: 1px solid #11A2B2;
  color: #11A2B2;
  background-color: transparent;
}
.cancel,
a.cancel {
  color: #666;
  background-color: #FFF;
  border-color: #CCC;
}
.confirm,
a.confirm {
  color: #FFF;
  background-color: #11A2B2;
  border-color: #11A2B2;
}
.btn-nocolor-sm,
a.btn-nocolor-sm {
  border: 1px solid #07AABC;
  padding: 3px 10px;
  color: #07AABC;
  background-color: transparent;
}
.btn-nocolor-lg,
a.btn-nocolor-lg {
  border: 1px solid #07AABC;
  padding: 5px 40px;
  color: #07AABC;
  background-color: transparent;
}
.btn-nocolor-xlg,
a.btn-nocolor-xlg {
  border: 2px solid #07AABC;
  padding: 15px 40px;
  color: #07AABC;
  font-size: 20px;
  background-color: transparent;
}
.btn-nocolor-sm:hover,
.btn-nocolor-sm:focus,
a.btn-nocolor-sm:hover,
a.btn-nocolor-sm:focus,
.btn-nocolor-lg:hover,
.btn-nocolor-lg:focus,
a.btn-nocolor-lg:hover,
a.btn-nocolor-lg:focus,
.btn-nocolor-xlg:hover,
.btn-nocolor-xlg:focus,
a.btn-nocolor-xlg:hover,
a.btn-nocolor-xlg:focus,
.underline-btn:hover,
.underline-btn:focus,
a.underline-btn:hover,
a.underline-btn:focus {
  color: #9cddf5;
}
/*datepicker*/
.uib-button-bar {
  .btn-group {
    button[ng-click^="select(null, $event)"] {
      display: none;
    }
  }
}
ul.dropdown-menu {
  max-height: 300px;
}
/*modal*/
.modal-header {
  font-size: 16px;
  color: #ffffff;
  background-color: #07AABC;
  .close {
    font-size: 28px;
    color: #333;
  }
}
.h40 {
  height: 40px;
}
.alert {
  width: 200px;
}

.back-white {
  background-color: #FFFFFF;
}
.back-gray {
  background-color: #F3F3F4;
}
.table > thead > tr > th {
  font-weight: 400;
  font-style: normal;
  background-color: #D1F0E9;
  border-bottom: 0;
}

/*nav steps*/
.steps-nav {
  > li {
    float: left;
    position: relative;
    width: 14%;
    padding-right: 10px;
  }
  > li span {
    display: block;
    padding-left: 25px;
    color: #999;
    background-color: #D7D7D7;
    line-height: 40px;
    font-size: 14px;
    text-align: center;
  }
  > li.actived {
    span {
      color: #FFFFFF;
      background-color: #00a2ca;
    }
  }
  >li:nth-child(odd) {
    span:after,
    span:before {
      content: " ";
      height: 0;
      width: 0;
      border-style: solid;
      border-width: 20px 5px 20px 25px;
      border-color: transparent;
      pointer-events: none;
    }
    span:before {
      position: absolute;
      left: 0;
      border-left-color: #F3F3F4;
    }
    span:after {
      position: absolute;
      right: -20px;
      z-index: 1;
      border-left-color: #D7D7D7;
    }
  }
  >li:nth-child(even) {
    height: 2px;
    margin-top: 20px;
    background-color: transparent;
    background-image: url(../images/dot_line.png);
    background-repeat-y: no-repeat;
  }
  >li.actived:nth-child(odd) {
    span:after {
      border-left-color: #00a2ca;
    }
  }
  >li.passed:nth-child(odd) {
    span:after {
      border-left-color: #9cddf5;
    }
  }
  >li.passed:nth-child(odd) {
    span {
      background-color: #9cddf5;
      color: #FFFFFF;
      cursor: pointer;
    }
  }
}

/*pagination*/
/*分页功能样式*/
.pagination {
  margin: 0;
}
.pagination-define {
  padding-bottom: 20px;
  input {
    width: 62px;
  }
  select,
  input {
    padding-left: 3px;
    height: 30px;
    vertical-align: top;
    border: 1px solid #ccc;
    border-radius: 4px;
  }
  .btn-30h {
    vertical-align: top;
    padding-top: 4px;
    padding-bottom: 4px;
  }
  .pagination > {
    .active > {
      a,
      a:hover,
      a:focus,
      span,
      span:hover,
      span:focus {
        background-color: #6bc6e8;
        border-color: #6bc6e8;
      }
    }
  }
}

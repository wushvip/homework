/*init browser css*/
body, html {width:100%;height:100%;margin:0;padding:0}
body{font-family:MicrosoftYaHei,Tahoma, Arial, sans-serif; color:#333; background-color:#FFF;font-size:15px;}
/*body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,legend,table,td,th,img,div{margin:0;padding:0;border:0;}*/
ul,ol{list-style-type:none;}
li{list-style:none;}
a:link{color:#36F;text-decoration:none;}
a:visited{color:#36F;}
a:hover{text-decoration:none;}
select,input,img,select{vertical-align:middle;}
.clearit{clear:both;}
table{border-collapse:collapse;}
.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
  vertical-align: middle;
  word-wrap: break-word;
  word-break: break-all;
}
.all-wrapper, .bcse-header {
  min-width: 960px;
}
.red {
  color: #D72524;
}
.text-tip {
  color: #9A9691;
  font-size: 12px;
}
.line {
  padding-left: 2px;
  padding-right: 2px;
  border-left: solid 1px #ccc;
}
/*icons style*/
.glyphicon-ok,
.glyphicon-ok-circle {
  color: #2CB64E;
}
.glyphicon-remove,
.glyphicon-chevron-up,
.glyphicon-chevron-down,
.glyphicon-exclamation-sign {
  color: #9A9A9A;
}

/*dropdown menu*/
ul.dropdown-menu {
  max-height: 150px;
  overflow: auto;
  > li {
    label {
      display: block;
      padding: 3px 20px;
      margin: 0;
      clear: both;
      font-weight: 400;
      line-height: 20px;
      white-space: nowrap;
      cursor: pointer;
      input {
        vertical-align: top;
      }
    }
  }
  > li:hover {
    background-color: #F5F5F5;
  }
}

/*button btn*/
.btn-full-width {
  width: 100%;
}
a.btn {
  color: #333;
}
a.btn-lg {
  color: #ffffff;
}
.btn:hover,
.btn:focus,
a.btn:hover,
a.btn:focus{

package com.chinamobile.cmss.bcse.app.bean;

import java.io.Serializable;

/** 
 * @ClassName: FieldInfoBean 
 * @Description: 字段信息，包括字段id，字段名称及字段类型，在高级配置模块获取字段信息时使用
 * @author: zhenglinfeng
 * @date: 2016年2月16日 上午9:53:06  
 */
public class FieldInfoBean implements Serializable{
	/**
	 * @fieldName: serialVersionUID
	 * @fieldType: long
	 * @Description: TODO
	 */
	private static final long serialVersionUID = 1L;
	private String id;
	private String fieldName;
	private String fieldType;
	private String isIndexed;
	private String isShownable;
	
	public FieldInfoBean() {
		super();
		// TODO Auto-generated constructor stub
	}
	public FieldInfoBean(String id, String fieldName, String fieldType) {
		super();
		this.id = id;
		this.fieldName = fieldName;
		this.fieldType = fieldType;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getFieldName() {
		return fieldName;
	}
	public void setFieldName(String fieldName) {
		this.fieldName = fieldName;
	}
	public String getFieldType() {
		return fieldType;
	}
	public void setFieldType(String fieldType) {
		this.fieldType = fieldType;
	}
	public String getIsIndexed() {
		return isIndexed;
	}
	public void setIsIndexed(String isIndexed) {
		this.isIndexed = isIndexed;
	}
	public String getIsShownable() {
		return isShownable;
	}
	public void setIsShownable(String isShownable) {
		this.isShownable = isShownable;
	}

}

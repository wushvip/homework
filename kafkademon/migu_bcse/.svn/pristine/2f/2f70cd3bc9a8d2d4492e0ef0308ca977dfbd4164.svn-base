package com.chinamobile.cmss.bcse.index.bean;

/**
 * 
 * @ClassName: TableField
 * @Description: 数据表每一列的属性
 * @author: jinjing
 * @date: 2016年2月16日 下午2:50:17
 */
public class TableField {

	private String type = ""; // 字段类型
	private String outTable = ""; // 关联的外表名称
	private String key = ""; // 是否是主键 0:否 1：是
	private String isMutivalue;
	private String srcField;

	final public String getKey() {
		return key;
	}

	final public void setKey(String key) {
		this.key = key;
	}

	final public String getType() {
		return type;
	}

	final public void setType(String type) {
		this.type = type;
	}

	final public String getOutTable() {
		return outTable;
	}

	final public void setOutTable(String outTable) {
		this.outTable = outTable;
	}

	public String getIsMutivalue() {
		return isMutivalue;
	}

	public void setIsMutivalue(String isMutivalue) {
		this.isMutivalue = isMutivalue;
	}

	public String getSrcField() {
		return srcField;
	}

	public void setSrcField(String srcField) {
		this.srcField = srcField;
	}

}

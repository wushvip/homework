package com.chinamobile.cmss.bcse.config.util;

import java.util.List;

/**
 * 写solrconfig文件需要的实体类
 * @ClassName: SearchHandler 
 * @Description: TODO
 * @author: Liusong
 * @date: 2017年3月7日 下午5:03:41
 */
public class SearchHandler {
	private String handlerName;
	private String qf;
	private String showField;
	
	private List<String> fqs;
	private String fl;
	
	public String getHandlerName() {
		return handlerName;
	}
	public void setHandlerName(String handlerName) {
		this.handlerName = handlerName;
	}
	public String getQf() {
		return qf;
	}
	public void setQf(String qf) {
		this.qf = qf;
	}
		
	public String getShowField() {
		return showField;
	}
	
	public void setShowField(String showField) {
		this.showField = showField;
	}
	
	
	public List<String> getFqs() {
		return fqs;
	}
	public void setFqs(List<String> fqs) {
		this.fqs = fqs;
	}
	public String getFl() {
		return fl;
	}
	public void setFl(String fl) {
		this.fl = fl;
	}
	
	
	public SearchHandler() {
		super();
	}
	
	public SearchHandler(String handlerName, String qf, String showField, List<String> fqs, String fl) {
		super();
		this.handlerName = handlerName;
		this.qf = qf;
		this.showField = showField;
		this.fqs = fqs;
		this.fl = fl;
	}
	
	
	
	
	
}
